rails generate model Material name:string description:text

rails generate model Color name:string description:text

rails generate model Manufacturer name:string description:text

rails generate model Printer name:string description:text manufacturer:references

rails generate model Nozzle name:string description:text manufacturer:references

rails generate model Filament name:string description:text manufacturer:references material:references

# Maybe!? just referencing to a setting from User_Setting to have a setting history without changing data, just update via creating a new setting entry everytime the settings change???
rails generate model Setting name:string description:text

rails generate model Filament_Color filament:references{required} color:references{required}

rails generate model User_Printer user:references{required} printer:references{required}

rails generate model User_Nozzle user:references{required} nozzle:references{required}

rails generate model User_Filament_Color user:references{required} filament_color:references{required}


rails generate model User_Setup user:references{required} user_printer:references{required} user_nozzle:references{required} user_filament_color:references{required} 

rails generate model User_Setting user:references{required} user_setup:references{required} 
